    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.5",
    "react-modal": "^3.15.1",
    "react-responsive-carousel": "^3.2.23",
    "react-stickynode": "^4.1.0",
